<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.carrie.lldiary.ui.AddListActivity">

   <android.support.v7.widget.RecyclerView
       android:id="@+id/rv_add"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginRight="16dp"
       android:layout_marginLeft="16dp"
       />

    <com.carrie.lldiary.fab.AddFloatingActionButton
        android:id="@+id/fab_add_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_plusIconColor="@color/white"
        fab:fab_colorNormal="@color/blue_semi_transparent"
        fab:fab_colorPressed="@color/blue_semi_transparent_pressed"
        android:layout_margin="16dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        fab:fab_stroke_visible="true"
        />


</RelativeLayout>
